<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="140.0" prefWidth="600.0" style="-fx-background-color: black;"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="GUI.Controller.CategoryController">
    <children>
        <Button fx:id="savebtn" layoutX="500.0" layoutY="101.0" mnemonicParsing="false" onAction="#save" text="Save"/>
        <Button fx:id="Exitbtn" layoutX="551.0" layoutY="101.0" mnemonicParsing="false" onAction="#Exit" text="Exit"/>
        <Label layoutX="31.0" layoutY="25.0" prefHeight="17.0" prefWidth="80.0" text="add category" textFill="WHITE"/>
        <TextField fx:id="textfiled" layoutX="31.0" layoutY="42.0" prefHeight="25.0" prefWidth="172.0"/>
    </children>
</AnchorPane>
